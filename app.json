{
  "expo": {
    "name": "TodoApp",
    "slug": "todoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e7a13775-4643-40c1-b7e5-086dd8c08fce"
      }
    },
    "owner": "tinamai",
    "plugins": [
        [
          "expo-font",
          {
            "fonts": ["./src/assets/fonts/PlayfairDisplay-Regular.ttf", 
            "./src/assets/fonts/PlayfairDisplay-Italic.ttf"]
          }
        ]
      ]
  }
}
